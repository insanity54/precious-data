{
  "name": "precious-data",
  "version": "1.0.0",
  "description": "Precious Memories TCG Data",
  "main": "index.js",
  "scripts": {
    "rip": "DEBUG=precious-data ./p-data.js rip -a",
    "dev": "DEBUG=precious-data supervisor --watch lib,api --extensions 'js' app",
    "jsdoc": "jsdoc2md ./lib/*.js > ./docs/index.md",
    "test": "ava 'test/*.test.js'",
    "fixtures": "tput reset && teenytest --helper ./test/helper.js 'test/fixtures.test.js'",
    "coverage": "NOCK_BACK_MODE=lockdown jest --coverage && coveralls < coverage/lconv.info",
    "sql": "sqlitebrowser ~/.config/precious-data/precious-data.sqlite",
    "start": "node app"
  },
  "keywords": [
    "precious",
    "memories",
    "tcg",
    "card",
    "database",
    "rip",
    "scrape",
    "p-memories"
  ],
  "ava": {
    "failWithoutAssertions": false,
    "verbose": true
  },
  "author": "Chris Grimmett <chris@grimtech.net>",
  "license": "Unlicense",
  "devDependencies": {
    "chai": "^4.3.6",
    "concurrently": "^7.0.0",
    "coveralls": "^3.1.1",
    "jsdoc-to-markdown": "^7.0.1",
    "nock": "^13.0.4",
    "nock-record": "^0.3.9",
    "supertest": "^6.2.2",
    "supervisor": "^0.12.0",
    "testdouble": "^3.16.4",
    "testdouble-nock": "^0.2.0"
  },
  "dependencies": {
    "ava": "^4.1.0",
    "axios": "^0.21.1",
    "better-sqlite3": "^7.5.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.2",
    "cheerio": "^1.0.0-rc.10",
    "cli-progress": "^3.10.0",
    "debug": "^4.2.0",
    "express": "^4.17.3",
    "fuse.js": "^6.4.1",
    "globby": "^11.0.3",
    "js-yaml": "^4.1.0",
    "memoizee": "^0.4.15",
    "mvp.css": "^1.8.0",
    "node-fetch": "^3.2.3",
    "ramda": "^0.27.1",
    "sharp": "^0.30.3",
    "swagger-express-mw": "^0.7.0",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-dist": "^4.6.2",
    "swagger-ui-express": "^4.3.0",
    "yargs": "^16.0.3"
  },
  "optionalDependencies": {
    "sql": "^0.78.0"
  }
}
